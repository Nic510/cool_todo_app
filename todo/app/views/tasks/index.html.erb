<h1>Tasks. Woooo</h1>
<ul>
	<% @tasks.each do |task| %>
	<li>
		<div>
			Content: <%=task.content%>
		</div>
		<div>
			Complete: <%=task.complete%>
		</div>
		<div>
			Created At: <%=task.created_at%>
		</div>
		 <%= button_to "New Task", new_task_path, :class => "button", method: :get  %>
		<%= button_to "Delete", task, method: :delete %>
	</li>
	<%end%>
</ul>